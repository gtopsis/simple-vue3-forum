  },
  "devDependencies": {
    "@babel/types": "^7.22.5",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/node": "^20.3.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.43.0",
    "eslint-plugin-vue": "^9.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~5.1.3",
    "vite": "^4.3.9",
